{
  "name": "websocket-king",
  "version": "5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "dotenvx run -f ./.env.dev -- vite",
    "preview": "dotenvx run -f ./.env.dev -- vite preview",
    "type-check": "tsc -b",
    "build:web": "dotenvx run -f ./.env.prod.web -- tsc -b && vite build",
    "build:chrome": "dotenvx run --env INLINE_RUNTIME_CHUNK=false && dotenvx run -f ./.env.prod.chrome -- tsc -b && vite build && node ./scripts/chrome.js",
    "build:electron": "dotenvx run --env INLINE_RUNTIME_CHUNK=false && dotenvx run -f ./.env.prod.electron -- tsc -b && vite build && node ./scripts/electron.js",
    "copy-env": "node ./scripts/copy-env",
    "sentry:source-maps": "node ./scripts/sentry",
    "start:storybook": "start-storybook -p 6006 -s public",
    "build:storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.4.0",
    "@sentry/browser": "^8.42.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.462.0",
    "prismjs": "^1.29.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.1",
    "react-icons": "^3.11.0",
    "react-portal": "^4.2.2",
    "react-redux": "^7.2.9",
    "react-simple-code-editor": "^0.14.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^5.3.11",
    "twin.macro": "^2.8.2",
    "use-resize-observer": "^9.1.0",
    "uuid": "^11.0.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.26.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.17.9",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^17.0.83",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^17.0.25",
    "@types/react-portal": "^4.0.7",
    "@types/react-redux": "^7.1.34",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "clsx": "^2.1.1",
    "electron": "^33.2.1",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "sass-embedded": "^1.81.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.7.2",
    "vite": "^6.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.ts",
      "preset": "styled-components",
      "dataTwProp": true,
      "debugPlugins": false,
      "debug": false
    }
  }
}
