{
  "name": "rockite",
  "version": "0.4",
  "description": "A modern, free and open-source WebSockets client",
  "private": true,
  "type": "module",
  "readme": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/psyirius/rockite.git"
  },
  "bugs": {
    "email": "psyirius@gmail.com",
    "url": "https://github.com/psyirius/rockite/issues"
  },
  "contributors": [
    {
      "name": "Tom Lerendu",
      "email": "tom@tomlerendu.uk",
      "url": "https://tomlerendu.uk"
    },
    {
      "name": "Psyirius",
      "email": "psyirius@gmail.com",
      "url": "https://psyirius.github.io"
    }
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0",
    "electron": ">=33.2.0"
  },
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "dev": "dotenvx run -f ./.env.dev -- vite dev",
    "preview": "dotenvx run -f ./.env.dev -- vite preview",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format:dry": "biome format",
    "format": "biome format --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "type-check": "tsc -b",
    "build:web": "dotenvx run -f ./.env.prod.web -- tsc -b && vite build",
    "build:chrome": "dotenvx run --env INLINE_RUNTIME_CHUNK=false && dotenvx run -f ./.env.prod.chrome -- tsc -b && vite build && tsx ./scripts/chrome.ts",
    "build:electron": "dotenvx run --env INLINE_RUNTIME_CHUNK=false && dotenvx run -f ./.env.prod.electron -- tsc -b && vite build && tsx ./scripts/electron.ts",
    "start:storybook": "storybook dev -p 6006 --disable-telemetry",
    "build:storybook": "storybook build --disable-telemetry",
    "scripts:nu-sample": "nu scripts/sample.nu 100",
    "copy-env": "tsx ./scripts/copy-env"
  },
  "dependencies": {
    "@fontsource/dm-sans": "^5.1.0",
    "@fontsource/figtree": "^5.1.1",
    "@fontsource/lexend": "^5.1.1",
    "@fontsource/manrope": "^5.1.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^1.9.7",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "motion": "^11.13.1",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-icons": "^5.4.0",
    "react-portal": "^4.2.2",
    "react-redux": "^7.2.9",
    "react-simple-code-editor": "^0.14.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "sonner": "^1.7.0",
    "styled-components": "^5.3.11",
    "twin.macro": "^3.4.1",
    "use-resize-observer": "^9.1.0",
    "usehooks-ts": "^3.1.0",
    "uuid": "^11.0.3",
    "vaul": "^1.1.1",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.2",
    "@dotenvx/dotenvx": "^1.28.0",
    "@storybook/addon-actions": "^8.4.6",
    "@storybook/addon-controls": "^8.4.6",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-links": "^8.4.6",
    "@storybook/node-logger": "^8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-vite": "8.4.6",
    "@storybook/test": "^8.4.6",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.17.9",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.13",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.3.1",
    "@types/react-portal": "^4.0.7",
    "@types/react-redux": "^7.1.34",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^33.2.1",
    "globals": "^15.12.0",
    "lightningcss": "^1.28.2",
    "lucide-react": "^0.465.0",
    "nushell": "^0.100.0",
    "phosphor-react": "^1.4.1",
    "postcss": "^8.4.49",
    "sass-embedded": "^1.82.0",
    "storybook": "8.4.6",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.36.0",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "vite": "^6.0.1",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.ts",
      "preset": "styled-components",
      "dataTwProp": true,
      "debugPlugins": false,
      "debug": false
    }
  }
}
